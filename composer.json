{
    "name": "enzaime/sms",
    "description": "Enzaime SMS",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahbub Rabbani",
            "email": "mahbub.rucse@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "twilio/sdk": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Enzaime\\Sms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enzaime\\Sms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enzaime\\Sms\\ServiceProvider"
            ],
            "aliases": {
                "EnzSms": "Enzaime\\Sms\\Facades\\EnzSms"
            }
        }
    },
    "require-dev": {
        "doctrine/dbal": "^3.3",
        "nunomaduro/phpinsights": "^2.1",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.5.10",
        "laravel/pint": "^1.20",
        "guzzlehttp/guzzle": "8.0.x-dev"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter",
        "test-w": "vendor\\bin\\phpunit",
        "test-wf": "vendor\\bin\\phpunit --filter"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
